compiler_yacc_decl_make_all: my_dsl_yacc.h
compiler_yacc_decl_clean:
	-$(DEL_FILE) my_dsl_yacc.h
my_dsl_yacc.h: grammar/my_dsl.y
	bison -d -t -d -p my_dsl -b my_dsl grammar/my_dsl.y
	 rm -f my_dsl_yacc.h my_dsl_yacc.cpp
	 mv -f my_dsl.tab.h my_dsl_yacc.h
	 mv -f my_dsl.tab.c my_dsl_yacc.cpp
	

compiler_yacc_impl_make_all: my_dsl_yacc.cpp
compiler_yacc_impl_clean:
	-$(DEL_FILE) my_dsl_yacc.cpp
my_dsl_yacc.cpp: grammar/my_dsl.y \
		my_dsl_yacc.h
	


compiler_lex_make_all: my_dsl_lex.cpp
compiler_lex_clean:
	-$(DEL_FILE) my_dsl_lex.cpp
my_dsl_lex.cpp: grammar/my_dsl.l
	flex -d -Pmy_dsl grammar/my_dsl.l
	 rm -f my_dsl_lex.cpp
	 mv -f lex.my_dsl.c my_dsl_lex.cpp
	

compiler_clean: compiler_yacc_decl_clean compiler_yacc_impl_clean compiler_lex_clean 

####### Compile

my_dsl.o: my_dsl.cpp my_dsl.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o my_dsl.o my_dsl.cpp

value.o: value.cpp value.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o value.o value.cpp

ast.o: ast.cpp ast.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o ast.o ast.cpp

expression.o: expression.cpp expression.h \
		ast.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o expression.o expression.cpp

statement.o: statement.cpp statement.h \
		ast.h \
		result_type.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o statement.o statement.cpp

result_type.o: result_type.cpp result_type.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o result_type.o result_type.cpp

interpretor.o: interpretor.cpp interpretor.h \
		variable.h \
		value.h \
		statement.h \
		ast.h \
		result_type.h \
		function.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o interpretor.o interpretor.cpp

variable.o: variable.cpp variable.h \
		value.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o variable.o variable.cpp

function.o: function.cpp function.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o function.o function.cpp

my_dsl_yacc.o: my_dsl_yacc.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o my_dsl_yacc.o my_dsl_yacc.cpp

my_dsl_lex.o: my_dsl_lex.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o my_dsl_lex.o my_dsl_lex.cpp

####### Install

install_target: first FORCE
	@test -d $(INSTALL_ROOT)/usr/lib || mkdir -p $(INSTALL_ROOT)/usr/lib
	-$(INSTALL_PROGRAM) "../lib/$(TARGET)" "$(INSTALL_ROOT)/usr/lib/$(TARGET)"
	-$(SYMLINK) "$(TARGET)" "$(INSTALL_ROOT)/usr/lib/$(TARGET0)"
	-$(SYMLINK) "$(TARGET)" "$(INSTALL_ROOT)/usr/lib/$(TARGET1)"
	-$(SYMLINK) "$(TARGET)" "$(INSTALL_ROOT)/usr/lib/$(TARGET2)"

uninstall_target: FORCE
	-$(DEL_FILE) "$(INSTALL_ROOT)/usr/lib/$(TARGET)" 
	 -$(DEL_FILE) "$(INSTALL_ROOT)/usr/lib/$(TARGET0)" 
	 -$(DEL_FILE) "$(INSTALL_ROOT)/usr/lib/$(TARGET1)" 
	 -$(DEL_FILE) "$(INSTALL_ROOT)/usr/lib/$(TARGET2)"
	-$(DEL_DIR) $(INSTALL_ROOT)/usr/lib/ 


install:  install_target  FORCE

uninstall: uninstall_target   FORCE

FORCE:

